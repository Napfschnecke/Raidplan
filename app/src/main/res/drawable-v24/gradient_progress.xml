<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/priest" />
        </shape>
    </item>

    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:startColor="@color/hunter"
                    android:centerColor="@color/dh"
                    android:endColor="@color/legendary"/>
            </shape>
        </clip>
    </item>
</layer-list>